<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="kr.co.hkcloud.palette3.chat.qa.dao.ChatQACommonRestMapper">

<select id="selectRtnBeforeMonthLastDay" parameterType="java.util.HashMap" resultType="java.util.HashMap">
SELECT TO_CHAR(LAST_DAY(ADD_MONTHS(TRUNC(NOW(),'MM'), -1 )), 'YYYYMMDD') AS BEFORE_MONTH_LAST_DAY 
</select>

</mapper>
